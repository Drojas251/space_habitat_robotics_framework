<root main_tree_to_execute = "MainTree">
    <BehaviorTree ID="MainTree">
        <Sequence>
            <SetBlackboard output_key="id_1" value="0" />
            <Fallback>
                <Sequence>
                    <SubTree ID="MoveRobot" target="id_1" />
                </Sequence>
            </Fallback>
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="MoveRobot">
        <Sequence name="SetGoal">
            <ClearOctomap/>
            <MoveToTarget target="step6" />
            <Wait sec="3" />
            <Pick object_id="cylinder" retreat="-x"/>
            <MoveGripperToTarget target="Open"/>
            <DetachObject object_id="cylinder"/>
            <EnableCamera enable="true" />
            <Pick object_id="cylinder"/>
            <Place object_id="cylinder" pose="0.4;0;0.1;0;0;0"/>
            <RemoveObject object_id="cylinder"/>
            <MoveGripper gripper_width="0.05" />  <!-- GRIPPER_POSITION_BROKEN -->
            <MoveToPose pose="0.2;0;0.5;0;0;0"/>
            
            <Reset/>
            
            
        </Sequence>
    </BehaviorTree>
</root>